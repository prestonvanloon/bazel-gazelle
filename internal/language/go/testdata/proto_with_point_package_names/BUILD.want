load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("@io_bazel_rules_go//proto:def.bzl", "go_proto_library")

proto_library(
    name = "baz_proto",
    srcs = ["foo.proto"],
    _gazelle_imports = [],
    visibility = ["//visibility:public"],
)

go_proto_library(
    name = "baz_go_proto",
    _gazelle_imports = [],
    importpath = "example.com/repo/proto_with_point_package_names",
    proto = ":foo_bar_baz_proto",
    visibility = ["//visibility:public"],
)

go_library(
    name = "go_default_library",
    _gazelle_imports = [],
    embed = [":baz_go_proto"],
    importpath = "example.com/repo/proto_with_point_package_names",
    visibility = ["//visibility:public"],
)
